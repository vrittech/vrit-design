  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vrit-design.umd.cjs",
  "module": "./dist/vrit-design.js",
  "exports": {
    ".": {
      "import": "./dist/vrit-design.js",
      "require": "./dist/vrit-design.umd.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/vrittech/vrit-design.git"
