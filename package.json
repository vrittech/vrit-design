    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "semantic-release": "^24.1.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
